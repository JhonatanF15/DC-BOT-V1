# Token do bot do Discord
TOKEN=seu_token_aqui

# ID do usuário autorizado
USER_ID=seu_user_id_aqui

# ID do canal para mensagens privadas
CANAL_ID=seu_canal_id_aqui

# Token secreto para autenticação (opcional)
SECRET_TOKEN=seu_token_secreto_aqui

# Ativar ou desativar o keepalive
KEEPALIVE_ENABLED=true

# Porta do servidor (Replit define automaticamente, mas pode ser especificada)
PORT=3000
